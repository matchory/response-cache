{
    "name": "matchory/response-cache",
    "description": "A package to add automatic response caching to Laravel APIs.",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "illuminate/support": "^v10.48|^11.11"
    },
    "require-dev": {
        "illuminate/auth": "^v10.48|^11.11",
        "illuminate/cache": "^v10.48|^11.11",
        "illuminate/contracts":"^v10.48|^11.11",
        "illuminate/http": "^v10.48|^11.11",
        "phpunit/phpunit": "^10.5|^11.2"
    },
    "autoload": {
        "psr-4": {
            "Matchory\\ResponseCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Matchory\\ResponseCache\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Matchory\\ResponseCache\\ResponseCacheProvider"
            ],
            "aliases": {
                "ResponseCache": "Matchory\\ResponseCache\\Facades\\ResponseCache"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Moritz Friedrich",
            "email": "moritz@matchory.com"
        }
    ]
}
